{
    "manifest_version": 3,
    "name": "SUPERHERO-IFY",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["./main.js"]
        }
      ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "Safeimagekit-resized-img.png"
    }
  }